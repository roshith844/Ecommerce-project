<h1>Users list</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">No</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
        </tr>
    </thead>
    <tbody>
        <% let count=1; %>
            <% users.forEach((user)=>{%>
                <tr>
                    <td>
                        <%= count %>
                    </td>
                    <td>
                        <%= user.email %>
                    </td>
                    <td>
                        <%= user.password %>
                    </td>
                </tr>
                <% count++; %>
                    <% }) %>
    </tbody>
</table>